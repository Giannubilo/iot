<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>DynamoDB Items</title>
</head>
<body>
    <h1>DynamoDB Items</h1>
    <div id="items-container"></div>

    <script>
        // Make a request to your API
        fetch('https://tb4zjp1wfi.execute-api.us-east-1.amazonaws.com/prod')
            .then(response => response.json())
            .then(data => {
                // Display the items on the HTML page
                const itemsContainer = document.getElementById('items-container');
                itemsContainer.innerHTML = '<pre>' + JSON.stringify(data, null, 2) + '</pre>';
            })
            .catch(error => console.error('Error:', error));
    </script>
</body>
</html>
